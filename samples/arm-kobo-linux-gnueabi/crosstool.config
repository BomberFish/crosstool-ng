CT_EXPERIMENTAL=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_STRIP_TARGET_TOOLCHAIN_EXECUTABLES=y
CT_ARCH_CPU="cortex-a8"
CT_ARCH_FPU="neon"
CT_ARCH_FLOAT_HW=y
CT_ARCH_arm=y
CT_ARCH_ARM_MODE_THUMB=y
# CT_ARCH_ARM_TUPLE_USE_EABIHF is not set
CT_TARGET_VENDOR="kobo"
CT_KERNEL_linux=y
CT_KERNEL_V_2_6_33=y
CT_CC_BINUTILS_SHOW_LINARO=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_LIBC_GLIBC_V_2_15=y
CT_LIBC_GLIBC_KERNEL_VERSION_CHOSEN=y
CT_LIBC_GLIBC_MIN_KERNEL_VERSION="2.6.33"
CT_CC_GCC_SHOW_LINARO=y
CT_CC_GCC_V_linaro_5_2=y
# CT_CC_GCC_USE_GRAPHITE is not set
# CT_CC_GCC_STATIC_LIBSTDCXX is not set
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE is not set
CT_CC_GCC_LNK_HASH_STYLE_GNU=y
CT_CC_LANG_CXX=y
